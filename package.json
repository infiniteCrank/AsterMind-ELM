{
  "name": "@astermind/astermind-elm",
  "version": "1.0.2",
  "description": "JavaScript Extreme Learning Machine (ELM) library for browser and Node.js.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    }
  },
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "postbuild": "cp dist/index.umd.js public/astermind.umd.js || true",
    "watch": "rollup -c -w",
    "clean": "rm -rf dist",
    "dev": "vite",
    "dev:autocomplete": "DEMO=autocomplete-chain vite",
    "dev:lang": "DEMO=language-awareness-demo vite",
    "dev:chain": "DEMO=chain-with-save vite",
    "dev:news": "DEMO=ag-news-demo vite",
    "dev:elm": "DEMO=elm-explination vite",
    "dev:music": "DEMO=music-generator vite",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infiniteCrank/AsterMind-ELM.git"
  },
  "bugs": {
    "url": "https://github.com/infiniteCrank/AsterMind-ELM/issues"
  },
  "homepage": "https://github.com/infiniteCrank/AsterMind-ELM#readme",
  "keywords": [
    "elm",
    "extreme-learning-machine",
    "ml",
    "javascript",
    "browser",
    "node"
  ],
  "author": "Julian Wilkison-Duran <clockworksyler@gmail.com>",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/csv-parse": "^1.1.12",
    "@types/node": "^24.0.3",
    "@types/yargs": "^17.0.33",
    "@xenova/transformers": "^2.17.2",
    "csv-parse": "^5.6.0",
    "eslint": "^9.29.0",
    "jsdom": "^26.1.0",
    "ml-matrix": "^6.12.1",
    "ml-pca": "^4.1.1",
    "plotly.js-dist": "^3.0.1",
    "prettier": "^3.5.3",
    "rollup": "^4.43.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsne-js": "^1.0.3",
    "typescript": "^5.9.2",
    "umap-js": "^1.4.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "yargs": "^18.0.0"
  }
}
